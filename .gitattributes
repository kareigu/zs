# Normalize EOL for all files that Git considers text files.
* text=auto eol=lf
*.png filter=lfs diff=lfs merge=lfs -text
*.blend filter=lfs diff=lfs merge=lfs -text
*.glb filter=lfs diff=lfs merge=lfs -text
*.occ filter=lfs diff=lfs merge=lfs -text
*.exr filter=lfs diff=lfs merge=lfs -text
*.lmbake filter=lfs diff=lfs merge=lfs -text
addons/godot-jolt/windows/**/* filter=lfs diff=lfs merge=lfs -text
addons/godot-jolt/ios/**/* filter=lfs diff=lfs merge=lfs -text
addons/godot-jolt/android/**/* filter=lfs diff=lfs merge=lfs -text
addons/godot-jolt/linux/**/* filter=lfs diff=lfs merge=lfs -text
addons/godot-jolt/macos/**/* filter=lfs diff=lfs merge=lfs -text
